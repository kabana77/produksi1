  object QInsertPengering0: TOracleQuery
    SQL.Strings = (
      'begin'
      '  '
      '  DELETE FROM ipisma_db3.item_saldo_dyeing'
      '  WHERE tahun=:ptahun AND pengelompokan='#39'PENGERING0'#39';'
      '  COMMIT;'
      ''
      '  INSERT INTO ipisma_db3.item_saldo_dyeing'
      '  (kd_item, kd_warna, tahun, b00, b01, pengelompokan)'
      '  SELECT'
      '    a.kd_item,'
      '    '#39'ALL'#39','
      '    :ptahun,'
      '    ((a.awal_thn + a.awal_tgl + a.qty_in) - a.qty_out),'
      '    ((a.awal_thn2 + a.awal_tgl2 + a.qty_in2) - a.qty_out2),'
      '    '#39'PENGERING0'#39
      '  FROM ('
      '    SELECT'
      '        kd_item,'
      '        SUM(awal_thn) AS awal_thn,'
      '        SUM(awal_thn2) AS awal_thn2,'
      '        SUM(awal_tgl) AS awal_tgl,'
      '        SUM(awal_tgl2) AS awal_tgl2,'
      '        SUM(qty_in) AS qty_in,'
      '        SUM(qty_in2) AS qty_in2,'
      '        SUM(qty_out) AS qty_out,'
      '        SUM(qty_out2) AS qty_out2'
      '    FROM IPISMA_DB3.TEMP_MUTASI_PENGERINGAN0 t'
      '    GROUP BY kd_item'
      ' ) a;'
      ' commit;'
      'end;')
    Session = DMFrm.OS
    Variables.Data = {0300000001000000070000003A50544148554E050000000000000000000000}
    Cursor = crSQLWait
    Left = 288
    Top = 31
