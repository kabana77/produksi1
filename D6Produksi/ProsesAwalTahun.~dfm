  object QGWarna2: TOracleQuery
    SQL.Strings = (
      'begin'
      '  '
      '  DELETE FROM ipisma_db3.item_saldo_gw'
      '  WHERE tahun=:ptahun;'
      '  COMMIT;'
      ''
      '  INSERT INTO ipisma_db3.item_saldo_gw'
      '  (kd_item, kd_warna, tahun, b00)'
      '  SELECT'
      '    a.kd_item,'
      '    a.kd_warna,'
      '    :ptahun,'
      '    ((a.awal_thn + a.awal_tgl + a.qty_in) - a.qty_out)'
      '  FROM ('
      '    SELECT'
      '      substr(kd_item, 1, 8) as kd_item,'
      '      substr(kd_item, 8, 3) as kd_warna,'
      '      SUM(awal_thn) AS awal_thn,'
      '      SUM(awal_tgl) AS awal_tgl,'
      '      SUM(qty_in) AS qty_in,'
      '      SUM(qty_out) AS qty_out'
      '    FROM IPISMA_DB3.TEMP_MUTASI_GW t'
      '    GROUP BY kd_item, kd_warna'
      '  ) a;'
      ''
      ' commit;'
      'end;')
